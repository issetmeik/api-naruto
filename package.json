    "build": "tsc",
    "test:watch": "jest --watch --verbose",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "jest-mock-extended": "^2.0.7",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": ">=13",
    "prettier": "^2.7.1",
    "prisma": "^4.11.0",
    "ts-jest": "^28.0.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
